{
  "name": "expense-manager",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "build": "next build",
    "deploy": "pnpm run build && vercel --prod",
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=8192 next dev",
    "lint:fix": "pnpm run lint --fix",
    "lint": "eslint --cache --ext js,ts,tsx . --max-warnings 0",
    "prettier:fix": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --cache",
    "prettier": "prettier --check \"./**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --cache",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "test:e2e:ci:build-app": "cross-env ENVIRONMENT=development next build",
    "test:e2e:ci:start-app": "cross-env ENVIRONMENT=development next start",
    "test:unit:watch": "pnpm run test:unit --watch",
    "test:unit": "jest --runInBand",
    "test": "pnpm run typescript && pnpm run test:unit && pnpm run test:e2e",
    "typescript:watch": "tsc -p . --watch",
    "typescript": "tsc -p .",
    "prepare": "husky install"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
