    "@nivo/line": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@nivo/sunburst": "^0.80.0",
    "@siegrift/tsfunct": "^2.0.5",
    "@visx/responsive": "^2.17.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "firebase": "^9.16.0",
    "firebase-admin": "^11.5.0",
    "js-beautify": "^1.14.7",
    "lodash": "^4.17.21",
